#!/usr/bin/env bash
alias activate="source ./.venv/bin/activate"
alias cdp="cd ~/projects"
alias d="docker"
alias dk="devkit"
alias dprune="bash ~/.zsh/scripts/docker-prune"
alias e="exit"
alias envs="printenv | grep ${1}"
alias ga="git add"
alias gb="git branch"
alias gbclean="git checkout main && git fetch && git branch --merged | egrep -v 'main|master|\bkeep\b' | xargs git branch -d"
alias gca="git commit --amend"
alias gcano="git commit --amend --no-edit"
alias gcm="git commit -m"
alias gco="git checkout"
alias gcom="git checkout main"
alias gfr="git fetch && git rebase"
alias gl="git log --pretty=short --graph"
alias gp="git push"
alias gpf="git push --force"
alias grm="git rebase main"
alias gstat="git status"
alias kill-port="bash ~/.zsh/scripts/kill-port"
alias ls="ls -la"
alias lzd="lazydocker"
alias md="make dev"
alias tf="terraform"
alias zsource="source ~/.zshrc"
